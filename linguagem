// Atribuir valor
var = var; || var = num;

// op lógicas
a > b	   // Maior
a < b	   // Menor
a == b	 // Igual
a ?= b	 // Diferente
a >= b	 // Maior ou igual
a <= b	 // Menor ou igual
not a    // Negação

// op aritiméticas
a + y    // Soma
a - y    // Subtração
a * y    // Multiplicação
a / y    // Divisão
a ** y	 // Potenciação

// Declaração de variáveis
int a, b = num, [...];

// Loops
for start, end
    // Código;
endfor

while (op)
    // Código;
endwhile

// Condicionais
if (op) then
    // código;
else
    // código;
endif

// Input e Output
show (var ou num);	// print var ou num
read var;	          // scan
